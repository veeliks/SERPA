{
	"name": "SERPA",
	"version": "0.1.0",
	"manifest_version": 3,
	"description": "Simulate website appearance in Google search",
	"author": "Felix Karhusaari",
	"permissions": ["activeTab", "scripting"],
	"icons": {
		"16": "icon_16.png",
		"32": "icon_32.png",
		"64": "icon_64.png",
		"128": "icon_128.png"
	},
	"action": {
		"default_popup": "index.html",
		"default_icon": {
			"16": "icon_16.png",
			"32": "icon_32.png",
			"64": "icon_64.png",
			"128": "icon_128.png"
		}
	}
}
